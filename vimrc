set runtimepath^=~/.vim/bundle/ctrlp.vim
syntax enable "enable syntax processing
set tabstop=4 
set softtabstop=4
set expandtab "tabs are spaces
set showcmd
set wildmenu "enables tab completition in certain cases
set showmatch "enables paranthesis/bracket highlighting
set incsearch "search as you enter characters
set foldenable 
set foldlevelstart=3
set ruler
